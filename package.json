    "drizzle-orm": "^0.28.6",
    "elysia": "^0.6.23",
    "pg": "^8.11.3"
  },
  "devDependencies": {
    "bun-types": "latest",
    "drizzle-kit": "^0.19.13"
  },
  "module": "src/index.js"
